<!DOCTYPE html>
<html lang="en">
<head>
    <!-- Google Tag Manager -->
    <script>
        (function (w, d, s, l, i) {
            w[l] = w[l] || []; w[l].push({
                'gtm.start':
                    new Date().getTime(), event: 'gtm.js'
            }); var f = d.getElementsByTagName(s)[0],
                j = d.createElement(s), dl = l != 'dataLayer' ? '&l=' + l : ''; j.async = true; j.src =
                    'https://www.googletagmanager.com/gtm.js?id=' + i + dl; f.parentNode.insertBefore(j, f);
        })(window, document, 'script', 'dataLayer', 'GTM-N3MXB6P');</script>
    <!-- End Google Tag Manager -->
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, viewport-fit=cover">
    <meta name="theme-color" content="#0b0c0c">

    @{
        <environment include="Development,DR,FDEV,ASIT,ESIT,PPRD">
            <meta name="robots" content="noindex, nofollow">
        </environment>
    }


    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <link rel="shortcut icon" sizes="16x16 32x32 48x48" href="/assets/images/favicon.ico" type="image/x-icon">
    <link rel="mask-icon" href="/assets/images/govuk-mask-icon.svg" color="#0b0c0c">
    <link rel="apple-touch-icon" sizes="180x180" href="/assets/images/govuk-apple-touch-icon-180x180.png">
    <link rel="apple-touch-icon" sizes="167x167" href="/assets/images/govuk-apple-touch-icon-167x167.png">
    <link rel="apple-touch-icon" sizes="152x152" href="/assets/images/govuk-apple-touch-icon-152x152.png">
    <link rel="apple-touch-icon" href="/assets/images/govuk-apple-touch-icon.png">

    @if (ViewData["Title"] != null && ViewData["ErrorTitle"] != null )
    {
     <title>
         @ViewData["ErrorTitle"] @ViewData["Title"] - Boiler Upgrade Scheme (BUS) - GOV.UK
     </title>
    }
    else if (ViewData["Title"] != null && ViewData["ErrorTitle"] == null)
    {
     <title>
         @ViewData["Title"]  - Boiler Upgrade Scheme (BUS) - GOV.UK
     </title>
   
    }
    else 
    {
     <title>
            Boiler Upgrade Scheme applications - GOV.UK
     </title>
    }

    <!--[if !IE 8]><!-->
    <link href="~/css/main.css" rel="stylesheet" asp-append-version="true" />
    <!--<![endif]-->
    <!--[if IE 8]>
      <link href="~/css/main-ie8.css" rel="stylesheet" asp-append-version="true" />
    <![endif]-->
    <!--[if lt IE 9]>
      <script src="~/js/html5shiv.js" asp-append-version="true"></script>
    <![endif]-->



</head>
<body class="govuk-template__body">
    <!-- Google Tag Manager (noscript) -->
    <noscript>
        <iframe title="googletagmanager" src="https://www.googletagmanager.com/ns.html?id=GTM-N3MXB6P"
                height="0" width="0" style="display:none;visibility:hidden"></iframe>
    </noscript>
    <!-- End Google Tag Manager (noscript) -->
    <script>document.body.className = ((document.body.className) ? document.body.className + ' js-enabled' : 'js-enabled');</script>
    <partial name="_CookieBanner" />
    <a href="#main-content" class="govuk-skip-link" data-module="govuk-skip-link">Skip to main content</a>

    <partial name="_Header.cshtml" , model="null" view-data="null" />

    <div class="govuk-width-container">
        <partial name="_BreadCrumbs.cshtml" , model="null" view-data="null" />

        <main id="main-content" role="main" class="govuk-main-wrapper govuk-main-wrapper--auto-spacing">
            @RenderBody()

            
        </main>

    </div>
    <partial name="_Footer.cshtml" , model="null" view-data="ViewData" />

    <script src="~/js/jquery.min.js" asp-append-version="true"></script>
    <script src="~/js/all.js" asp-append-version="true"></script>
    <script src="~/js/custom.min.js" asp-append-version="true"></script>


    @await RenderSectionAsync("Scripts", required: false)
    <script>
        window.GOVUKFrontend.initAll()
    </script>
</body>
</html>